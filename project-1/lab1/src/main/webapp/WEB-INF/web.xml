<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>ChaikovskaServlet</servlet-name>
        <jsp-file>/chaikovska.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChaikovskaServlet</servlet-name>
        <url-pattern>/chaikovska</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SliusarevaServlet</servlet-name>
        <jsp-file>/sliusareva.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>SliusarevaServlet</servlet-name>
        <url-pattern>/sliusareva</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LykhohubServlet</servlet-name>
        <jsp-file>/lykhohub.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>LykhohubServlet</servlet-name>
        <url-pattern>/lykhohub</url-pattern>
    </servlet-mapping>
</web-app>